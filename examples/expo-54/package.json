{
  "name": "expo-key-event-example-expo-54",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "e2e:server": "node e2e/server.js",
    "e2e:web": "maestro test e2e/browser.e2e.yaml --headless",
    "e2e:android": "maestro test e2e/maestro.e2e.yaml",
    "e2e:ios": "maestro test e2e/maestro.e2e.yaml",
    "e2e:android:prepare": "expo prebuild --clean && npm run android -- --no-bundler",
    "e2e:ios:prepare": "expo prebuild --clean && npm run ios -- --no-bundler",
    "e2e:ios:prepare:ci": "expo prebuild --clean && ./e2e/ci-ios-setup.sh",
    "e2e:ios:prepare:preboot": "expo prebuild --clean && ./e2e/pre-boot-simulator.sh && npm run ios -- --no-bundler",
    "e2e:ios:all": "npm run e2e:ios:prepare && concurrently --kill-others 'npm run e2e:server' 'npx expo start' 'npm run e2e:ios'",
    "e2e:ios:all:ci": "npm run e2e:ios:prepare:ci && concurrently --kill-others 'npm run e2e:server' 'npx expo start' 'npm run e2e:ios'",
    "e2e:ios:all:preboot": "npm run e2e:ios:prepare:preboot && concurrently --kill-others 'npm run e2e:server' 'npx expo start' 'npm run e2e:ios'",
    "e2e:android:all": "npm run e2e:android:prepare && concurrently --kill-others 'npm run e2e:server' 'npx expo start' 'npm run e2e:android'",
    "e2e:web:all": "concurrently --kill-others 'npm run start' 'npm run e2e:web'"
  },
  "dependencies": {
    "expo": "~54.0.27",
    "expo-dev-client": "~6.0.20",
    "expo-status-bar": "~3.0.9",
    "express": "^4.19.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": "../.."
    }
  }
}
